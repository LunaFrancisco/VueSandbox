    "@vue/test-utils": "^2.0.2",
    "@vue/tsconfig": "^0.1.3",
    "eslint": "^8.5.0",
    "eslint-plugin-vue": "^9.0.0",
    "jsdom": "^20.0.0",
    "naive-ui": "^2.31.0",
    "npm-run-all": "^4.1.5",
    "typescript": "~4.7.4",
    "vfonts": "^0.0.3",
